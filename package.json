{
  "name": "vite-react-motoko",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "backend": "mo-dev --generate --deploy -y",
    "build": "tsc && vite build",
    "deploy:ic": "dfx deploy --ic",
    "deploy": "dfx deploy",
    "dfx": "dfx start --clean --host 127.0.0.1:8000",
    "format": "prettier --write \"src/**/*.{json,js,jsx,ts,tsx,css,scss}\" \"backend/**/*.mo\"",
    "frontend": "vite --port 3000",
    "postinstall": "mops install",
    "presetup": "dfx deps pull",
    "setup": "npm i && dfx canister create backend && dfx generate backend && dfx deploy",
    "sources": "mops sources",
    "start": "run-p frontend backend",
    "test:backend": "mo-test",
    "test:frontend": "vitest run",
    "test": "run-s test:backend test:frontend"
  },
  "dependencies": {
    "@headlessui/react": "^2.1.4",
    "@heroicons/react": "^2.1.5",
    "@ic-reactor/react": "^1.8.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-router-dom": "^6.26.1"
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^6.5.0",
    "@testing-library/react": "^16.0.1",
    "@types/react": "^18.3.5",
    "@types/react-dom": "^18.3.0",
    "@vitejs/plugin-react": "^4.3.1",
    "autoprefixer": "^10.4.20",
    "cross-fetch": "^4.0.0",
    "daisyui": "^4.12.10",
    "dotenv": "^16.3.1",
    "ic-mops": "^0.45.3",
    "jsdom": "^25.0.0",
    "mo-dev": "^0.13.0",
    "npm-run-all": "^4.1.5",
    "postcss": "^8.4.44",
    "prettier": "^3.3.3",
    "prettier-plugin-motoko": "^0.9.4",
    "sass": "^1.77.8",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^5.4.3",
    "vite-plugin-environment": "^1.1.3",
    "vitest": "^2.0.5"
  }
}
