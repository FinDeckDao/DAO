{
  "canisters": {
    "backend": {
      "declarations": { "node_compatibility": true },
      "dependencies": ["token"],
      "main": "backend/main.mo",
      "type": "motoko"
    },
    "frontend": {
      "dependencies": ["backend"],
      "source": ["dist/"],
      "type": "assets"
    },
    "internet_identity": {
      "type": "pull",
      "frontend": {},
      "id": "rdmx6-jaaaa-aaaaa-aaadq-cai"
    },
    "token": {
      "type": "pull",
      "id": "jaamb-mqaaa-aaaaj-qa3ka-cai"
    }
  },
  "defaults": {
    "build": {
      "packtool": "npm run --silent sources"
    }
  },
  "output_env_file": ".env",
  "version": 2,
  "networks": {
    "ic": {
      "providers": ["https://ic0.app", "https://mainnet.dfinity.network"]
    },
    "local": {
      "bind": "127.0.0.1:8000",
      "type": "ephemeral",
      "replica": {
        "subnet_type": "system"
      }
    }
  }
}
